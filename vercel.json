{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.js|.*\\.css)$",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
